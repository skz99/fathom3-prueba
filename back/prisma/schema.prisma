// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("CONNECTION")
}

model User {
  id      Int      @id @default(autoincrement())
  email   String   @unique
  name    String
  surname String
  default_lang String
  password String
}